select
	o_orderpriority,
	count(*) as order_count
from
	dbo.orders
where
	o_orderdate >=  '1993-07-01'
	and o_orderdate < DateAdd(month, 3, '1993-07-01')
	and exists 
	(
		select
		*
		from
		dbo.lineitem
		where
		l_orderkey = o_orderkey
		and l_commitdate < l_receiptdate
	)
group by
	o_orderpriority
order by
	o_orderpriority;